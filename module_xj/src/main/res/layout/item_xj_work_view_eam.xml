<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="@dimen/dp_15"
    android:paddingLeft="@dimen/dp_10"
    android:paddingRight="@dimen/dp_10"
    >
    <TextView
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="@drawable/sh_xj_work_eam_num"
        android:textColor="@color/white"
        android:textSize="@dimen/fontSize_14sp"
        android:textStyle="bold"
        android:text="11"
        android:gravity="center"
        android:id="@+id/itemXJWorkViewEamNum"
        android:layout_marginTop="@dimen/dp_5"
        android:tag="itemXJWorkViewEamNum" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:textColor="@color/xjTitleColor"
        android:textSize="@dimen/fontSize_16sp"
        android:text="设备11"
        android:gravity="center_vertical"
        android:id="@+id/itemXJWorkViewEamName"
        android:layout_toRightOf="@+id/itemXJWorkViewEamNum"
        android:layout_marginLeft="@dimen/dp_5"
        android:tag="itemXJWorkViewEamName" />

</RelativeLayout>